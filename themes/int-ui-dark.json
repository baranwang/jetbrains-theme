{
  "$schema": "vscode://schemas/color-theme",
  "name": "JetBrains New UI Dark",
  "type": "dark",
  "colors": {
    "focusBorder": "#0000",
    "icon.foreground": "#CED0D6",

    "editor.background": "#1E1F22",
    "editor.foreground": "#BCBEC4",
    "editor.selectionBackground": "#154288",
    "editor.lineHighlightBackground": "#26282E",
    "editor.findMatchBackground": "#2d543f",
    "editor.findMatchHighlightBackground": "#114957",

    "editorGroup.border": "#1E1F22",
    "editorGroupHeader.tabsBackground": "#1E1F22",
    "editorGroupHeader.tabsBorder": "#393B40",

    "editorLineNumber.foreground": "#4E5157",
    "editorLineNumber.activeForeground": "#9DA0A8",

    "editorGutter.modifiedBackground": "#375FAD",
    "editorGutter.addedBackground": "#549159",
    "editorGutter.deletedBackground": "#868A91",
    "editorGutter.foldingControlForeground": "#6F737A",

    "list.activeSelectionBackground": "#2E436E",
    "list.inactiveSelectionBackground": "#43454A",
    "list.activeSelectionForeground": "#DFE1E5",
    "list.inactiveSelectionForeground": "#DFE1E5",

    "statusBar.background": "#2B2D30",
    "statusBar.foreground": "#9DA0A8",
    "statusBar.border": "#1E1F22",
    "statusBar.noFolderBackground": "#2B2D30",
    "statusBar.debuggingBackground": "#2B2D30",
    "statusBarItem.remoteBackground": "#3574F0",
    "statusBarItem.remoteForeground": "#FFFFFF",

    "badge.background": "#3574F0",
    "badge.foreground": "#FFFFFF",
    "activityBarBadge.background": "#3574F0",
    "activityBarBadge.foreground": "#FFFFFF",

    "sideBar.background": "#2B2D30",
    "sideBar.foreground": "#DFE1E5",
    "sideBar.border": "#1E1F22",
    "sideBarSectionHeader.background": "#2B2D30",
    "sideBarSectionHeader.foreground": "#DFE1E5",

    "activityBar.background": "#2B2D30",
    "activityBar.foreground": "#CED0D6",
    "activityBar.border": "#1E1F22",
    "activityBar.activeBackground": "#4E5157",
    "activityBar.activeFocusBorder": "#0000",
    "activityBar.activeBorder": "#0000",
    "activityBar.inactiveForeground": "#737580",
    "activityBar.dropBorder": "#588CF3",

    "tab.activeBackground": "#1E1F22",
    "tab.activeForeground": "#DFE1E5",
    "tab.inactiveBackground": "#1E1F22",
    "tab.inactiveForeground": "#DFE1E5AA",
    "tab.activeBorder": "#3574F0",
    "tab.unfocusedActiveBorder": "#5A5D63",
    "tab.border": "#0000",

    "button.background": "#3574F0",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#366ACF",
    "button.border": "#0000",
    "button.secondaryBackground": "#0000",
    "button.secondaryForeground": "#DFE1E5",
    "button.separator": "#6B9BFA",

    "dropdown.background": "#2B2D30",
    "dropdown.border": "#4E5157",
    "dropdown.foreground": "#DFE1E5",

    "panel.background": "#2B2D30",
    "panel.border": "#1E1F22",
    "panelTitle.activeForeground": "#DFE1E5",
    "panelTitle.activeBorder": "#3574F0",
    "panelTitle.inactiveForeground": "#DFE1E5AA",

    "scrollbarSlider.background": "#48494B",
    "scrollbarSlider.hoverBackground": "#595A5C",

    "terminal.background": "#1E1F22",
    "terminal.foreground": "#BCBEC4",

    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#595959",
    "terminal.ansiRed": "#f75464",
    "terminal.ansiGreen": "#6aab73",
    "terminal.ansiYellow": "#e0bb65",
    "terminal.ansiBlue": "#56a8f5",
    "terminal.ansiMagenta": "#c77dbb",
    "terminal.ansiCyan": "#2aacb8",
    "terminal.ansiWhite": "#bcbec4",
    "terminal.ansiBrightRed": "#fa6675",
    "terminal.ansiBrightGreen": "#73bd79",
    "terminal.ansiBrightYellow": "#f2c55c",
    "terminal.ansiBrightBlue": "#548af7",
    "terminal.ansiBrightMagenta": "#c77dba",
    "terminal.ansiBrightCyan": "#42c3d4",
    "terminal.ansiBrightWhite": "#ced0d6",

    "editorBracketHighlight.foreground1": "#E8BA36",
    "editorBracketHighlight.foreground2": "#54A857",
    "editorBracketHighlight.foreground3": "#359FF4",
    "editorBracketHighlight.foreground4": "#6E7ED9",
    "editorBracketHighlight.foreground5": "#179387",

    "editorBracketHighlight.unexpectedBracket.foreground": "#FA6675",

    "editorError.foreground": "#FA6675",
    "editorInfo.foreground": "#7EC482",

    "merge.currentHeaderBackground": "#447152",
    "merge.currentContentBackground": "#294436",
    "merge.incomingContentBackground": "#385570",
    "merge.incomingHeaderBackground": "#43698D",

    "diffEditor.insertedLineBackground": "#294436",
    "diffEditor.insertedTextBackground": "#447152",
    "diffEditorGutter.insertedLineBackground": "#294436",
    "diffEditor.removedLineBackground": "#45302B",
    "diffEditor.removedTextBackground": "#8F5247",
    "diffEditorGutter.removedLineBackground": "#45302B",

    "gitlens.graphLane1Color": "#35538F",
    "gitlens.graphLane2Color": "#436946",
    "gitlens.graphLane3Color": "#826A41",
    "gitlens.graphLane4Color": "#7A4343",
    "gitlens.graphLane5Color": "#825845",
    "gitlens.graphLane6Color": "#583D7A",
    "gitlens.graphLane7Color": "#20635D"
  },
  "tokenColors": [
    {
      "scope": ["comment", "string.comment"],
      "settings": {
        "foreground": "#7a7e85"
      }
    },
    {
      "scope": ["comment.block.documentation", "comment.block.javadoc"],
      "settings": {
        "foreground": "#67a37c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment.block.documentation storage.type"],
      "settings": {
        "foreground": "#68A67E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "meta.type",
        "support.type",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined",
        "variable.language.this",
        "entity.name.command",
        "meta.structure.dictionary.value constant.language.json",
        "entity.name.tag.yaml",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#cf8e6d"
      }
    },
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#14C4B6"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.enummember",
        "variable.language",
        "meta.object-literal.key",
        "meta.definition.property",
        "meta.definition.property entity.name.function",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#c77dbb"
      }
    },
    {
      "scope": ["string", "entity.name.tag", "punctuation.definition.string"],
      "settings": {
        "foreground": "#6aab73"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#2aacb8"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes",
        "variable.other.object",
        "entity.name.type",
        "support.type.property-name",
        "constant.language.import-export-all",
        "string variable",
        "source.yaml string.unquoted",
        "source.yaml constant.language.boolean"
      ],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "scope": [
        "variable.other.link",
        "string.other.link",
        "constant.other.reference.link"
      ],
      "settings": {
        "foreground": "#548AF7",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "storage.type.annotation",
        "punctuation.definition.entity",
        "variable.interpolation",
        "meta.property-list",
        "entity.other.attribute-name.class",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#b3ae60"
      }
    },
    {
      "scope": ["entity", "entity.name.function", "meta.function-call.generic"],
      "settings": {
        "foreground": "#56a8f5"
      }
    },
    {
      "name": "Static members",
      "scope": ["entity.name.function.static"],
      "settings": {
        "foreground": "#57aaf7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Static properties",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#c77dbb",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": ["entity.name.tag.html", "entity.name.tag.xml"],
      "settings": {
        "foreground": "#d5b778"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#bcbec4"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#6aab73"
      }
    },
    {
      "name": "CSS Colors",
      "scope": ["constant.other.color.rgb-value.css"],
      "settings": {
        "foreground": "#56a8f5"
      }
    },
    {
      "name": "CSS Important",
      "scope": ["keyword.other.important.css"],
      "settings": {
        "foreground": "#cf8e6d",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#bcbec4",
    "type": "#bcbec4",
    "class": "#bcbec4",
    "enum": "#bcbec4",
    "interface": "#16baac",
    "struct": "#bcbec4",
    "typeParameter": "#16baac",
    "parameter": "#bcbec4",
    "variable": {
      "foreground": "#bcbec4",
      "fontStyle": ""
    },
    "property": "#c77dbb",
    "enumMember": "#c77dbb",
    "event": "#56a8f5",
    "function": "#57aaf7",
    "method": "#57aaf7",
    "macro": "#cf8e6d",
    "keyword": "#cf8e6d",
    "modifier": "#cf8e6d",
    "comment": "#7a7e85",
    "string": "#6aab73",
    "number": "#2aacb8",
    "regexp": "#42c3d4",
    "operator": "#bcbec4",
    "decorator": "#b3ae60"
  }
}
