{
  "name": "vscode-theme-jetbrains-new-ui",
  "version": "1.1.0",
  "publisher": "baran-wang",
  "engines": {
    "vscode": "^1.12.0"
  },
  "license": "MIT",
  "displayName": "Int UI Theme",
  "description": "Introducing a vibrant and enriched theme inspired by JetBrains' new Int UI. Packed with an array of vivid colors and an extensive collection of icons, it transforms and enhances your user interface.",
  "categories": [
    "Themes"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1E1F22",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baranwang/jetbrains-theme"
  },
  "scripts": {
    "prepare": "npm run build && npm run svgo",
    "build": "node ./scripts/icon.js",
    "svgo": "svgo -f ./icons -o ./icons",
    "vsce": "vsce"
  },
  "contributes": {
    "themes": [
      {
        "label": "JetBrains New UI Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/jetbrains-dark.json"
      }
    ],
    "iconThemes": [
      {
        "label": "JetBrains Icons Dark",
        "id": "jetbrains-icons-dark",
        "path": "./themes/jetbrains-dark-icons.json"
      },
      {
        "label": "JetBrains Icons Light",
        "id": "jetbrains-icons-light",
        "path": "./themes/jetbrains-light-icons.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.20.0",
    "jsonc-parser": "^3.2.0",
    "svgo": "^3.0.2"
  }
}
